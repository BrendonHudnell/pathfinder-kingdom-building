{
  "name": "react-express-typescript-boilerplate",
  "version": "1.0.0",
  "description": "full stack typescript react/express boilerplate",
  "repository": "https://github.com/BrendonHudnell/react-express-typescript-boilerplate",
  "author": "Brendon Hudnell",
  "license": "MIT",
  "engines": {
    "node": "14.x",
    "yarn": "1.x"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@reduxjs/toolkit": "^1.6.0",
    "ajv": "^8.6.0",
    "ajv-formats": "^2.1.0",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "ky": "^0.28.5",
    "mysql2": "^2.2.5",
    "react": "^17.0.1",
    "react-dnd": "^14.0.2",
    "react-dnd-html5-backend": "^14.0.0",
    "react-dom": "^17.0.1",
    "react-hexgrid": "^1.0.3",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.1.0",
    "typeorm": "^0.2.34"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/node": "^15.12.2",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-router-dom": "^5.1.7",
    "@types/sinon": "^10.0.2",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^8.0.0",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^5.2.6",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "extract-css-chunks-webpack-plugin": "^4.9.0",
    "html-webpack-plugin": "^5.0.0-beta.6",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "postcss": "^8.3.2",
    "postcss-loader": "^6.1.0",
    "prettier": "^2.3.1",
    "sass": "^1.34.1",
    "sass-loader": "^12.1.0",
    "sinon": "^11.1.1",
    "sqlite3": "^5.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "scripts": {
    "build": "yarn build:client && yarn build:server",
    "build:client": "cd ./client && webpack --config webpack.prod.js",
    "build:server": "tsc",
    "start": "node server/build/server.js",
    "dev:client": "cd ./client && webpack serve --open --config webpack.dev.js",
    "dev:server": "ts-node-dev --respawn --transpile-only server/src/server.ts",
    "lint": "eslint --ext .js,.ts,.tsx --quiet --fix .",
    "test": "yarn test:server",
    "test:server": "jest",
    "cover": "yarn cover:server",
    "cover:server": "jest --coverage",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --quiet --fix"
  }
}
